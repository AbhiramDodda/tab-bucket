{
  "manifest_version": 3,
  "name": "Tab Bucket",
  "version": "1.1",
  "description": "Copy, share and open multiple links with ease.",
  "permissions": [
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16x16.png",
      "48": "images/icon48x48.png",
      "128": "images/icon128x128.png",
      "256": "images/icon256x256.png"
    }
  },
  "icons": {
    "16": "images/icon16x16.png",
    "48": "images/icon48x48.png",
    "128": "images/icon128x128.png",
    "256": "images/icon256x256.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
